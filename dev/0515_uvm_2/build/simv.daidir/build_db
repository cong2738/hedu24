#!/bin/sh -e
# This file is automatically generated by VCS.  Any changes you make
# to it will be overwritten the next time VCS is run.
vcs '-full64' '-sverilog' '-ntb_opts' 'uvm-1.2' '-debug_access+all' '-kdb' '-timescale=1ns/100ps' '-Mdir=./build/csrc' '-o' './build/simv' '-l' './build/comp.log' './rtl/adder.sv' './tb/tb_adder.sv'  -static_dbgen_only -daidir=$1 2>&1
